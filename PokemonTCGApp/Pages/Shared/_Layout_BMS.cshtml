<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>

    <!-- jquery -->
    <script src="https://code.jquery.com/jquery-3.1.1.min.js" crossorigin="anonymous"></script>

    <!-- Bootstrap 3.3.5 -->
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css">
    @*<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">*@

    <!-- Vue 2.6.14 -->
    <script src="https://cdn.jsdelivr.net/npm/vue@2.6.14/dist/vue.js"></script>

    <!-- element樣式 -->
    <link rel="stylesheet" href="https://unpkg.com/element-ui/lib/theme-chalk/index.css">
    <!-- element組件庫 -->
    <script src="https://unpkg.com/element-ui/lib/index.js"></script>

    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.css">

    <!-- Ionicons -->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">

    <!-- AdminLTE App -->
    <link rel="stylesheet" href="~/lib/AdminLTE/dist/css/AdminLTE.css" />
    <!-- AdminLTE Skins -->
    <link rel="stylesheet" href="~/css/skins/_all-skins.min.css">

    <!-- bootstrap wysihtml5 - text editor -->
    <link rel="stylesheet" href="~/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.min.css">

    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap-grid.css" />

    <!-- semantic-ui -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/semantic-ui@2.4.2/dist/semantic.min.css">
    <script src="https://cdn.jsdelivr.net/npm/semantic-ui@2.4.2/dist/semantic.min.js"></script>

    @*<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />*@
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/PokemonTCGApp.styles.css" asp-append-version="true" />

    <!-- day.js -->
    <script src="https://unpkg.com/dayjs@1.8.21/dayjs.min.js"></script>

     <!-- lodash.js -->
    <script src="https://cdn.jsdelivr.net/npm/lodash@4.17.15/lodash.min.js"></script>

    <style>
        body, h1, h2, h3, h4, h5, h6, .h1, .h2, .h3, .h4, .h5, .h6, p, span, b {
            font-family: 微軟正黑體;
        }
    </style>
</head>
<body class="sidebar-mini wysihtml5-supported skin-yellow">
        <div class="wrapper">
            <header class="main-header">
                <!-- Logo -->
                <a href="#" class="logo">
                    <!-- mini logo for sidebar mini 50x50 pixels -->
                    <span class="logo-mini"><b>S</b>GP</span>
                    <!-- logo for regular state and mobile devices -->
                    <span class="logo-lg">SGP</span>
                </a>
                <!-- Header Navbar: style can be found in header.less -->
                <nav class="navbar navbar-static-top" role="navigation">
                    <!-- Sidebar toggle button-->
                    <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                        <span class="sr-only">Toggle navigation</span>
                    </a>
                    <div class="navbar-custom-menu">
                        <ul class="nav navbar-nav">

                            <!-- User Account: style can be found in dropdown.less -->
                            <li class="dropdown user user-menu">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                    <img src="~/images/blogo.png" class="user-image" alt="User Image" />
                                    <span class="hidden-xs">管理員 </span>
                                </a>
                                <ul class="dropdown-menu">
                                    <!-- User image -->
                                    <li class="user-header">
                                        <img src="~/images/blogo.png" class="img-circle" alt="User Image" />
                                        <p>
                                            管理員
                                            <small></small>
                                        </p>
                                    </li>

                                    <!-- Menu Footer-->
                                    <li class="user-footer">

                                        <div class="pull-right">
                                            @*<button ID="btnLogout" CssClass="btn btn-default btn-flat">登出</button>*@
                                            <button type="button" class="btn btn-primary" @*v-onclick="setFilter"*@>登出</button>
                                        </div>
                                    </li>
                                </ul>
                            </li>

                        </ul>
                    </div>
                </nav>
            </header>

            <aside class="main-sidebar sidebar-dark-primary elevation-4">

                @*<a href="../Pages/BMS/Index.cshtml" class="brand-link">
                    <img src="~/images/blogo.png" alt="SGP" class="img-circle" style="opacity: .8"/>
                    <span class="brand-text font-weight-light">SGP</span>
                </a>*@

                <!-- sidebar: style can be found in sidebar.less -->
                <section class="sidebar">

                    <!-- /.search form -->
                    <!-- sidebar menu: : style can be found in sidebar.less -->
                    <ul class="sidebar-menu">
                        <li class="header" id="MerchantPurchase">卡牌區</li>
                        <li id="li_MPdelivery">
                            <a asp-page="/BMS/CardList">
                                <i class="nintendo switch icon"></i><span>卡牌列表</span>
                                @*通知用*@
                                @*<small class="label pull-right bg-yellow" id="voucherExchangeCount"></small>*@
                            </a>
                        </li>
                        <li id="">
                            <a asp-page="/BMS/SetList">
                                <i class="chart pie icon"></i><span>上市系列設定及列表</span>
                            </a>
                        </li>
                        <li id="li_MPproducts">
                            <a href="">
                                <i class="fa  fa-cog"></i><span>卡片規格設定</span>
                            </a>
                        </li>
                        <li id="li_vuetable">
                            <a asp-page="/BMS/vuetableShow">
                                <i class="fa  fa-cog"></i><span>vuetable2.0演示</span>
                            </a>
                        </li>

                        <li class="header">卡組區</li>
                        
                        <li id="li_refundlist">
                            <a href="">
                                <i class="fa fa-cog"></i><span>卡組設定</span>
                            </a>
                        </li>

                        <li class="header">戰報區</li>
                        <li id="li_orderlistall">
                            <a href="">
                                <i class="steam icon"></i><span>戰報列表</span>
                            </a>
                        </li>
                        <li id="li_orderlistall">
                            <a href="">
                                <i class="fa fa-cog"></i><span>戰報規格設定</span>
                            </a>
                        </li>

                        <li class="header">會員專區</li>
                        <li id="li_orderlistall">
                            <a href="">
                                <i class="users icon"></i><span>會員列表</span>
                            </a>
                        </li>
                        <li id="li_orderlistall">
                            <a href="">
                                <i class="fa fa-cog"></i><span>會員規格設定</span>
                            </a>
                        </li>

                        <li class="header">管理員區</li>
                        <li id="li_orderlistall">
                            <a href="">
                                <i class="user secret icon"></i><span>管理員列表</span>
                            </a>
                        </li>
                        <li id="li_orderlistall">
                            <a href="">
                                <i class="fa fa-cog"></i><span>管理員規格設定</span>
                            </a>
                        </li>
                        
                    </ul>
                </section>
                <!-- /.sidebar  -->
            </aside>
        <div>
            @*<main role="main" class="pb-3">*@
            <main>
                @RenderBody()
            </main>
         </div>
    </div>

       
    <footer class="main-footer">
        <div class="pull-right hidden-xs">
            <b>Version</b> 1.0.0
        </div>
        <strong>Copyright &copy; - PokemonTCGApp - Created By SGP_Yen, SGP_Soda</strong>
        <a asp-area="" asp-page="/Privacy">Privacy</a>
    </footer>


         <!-- jQuery UI 1.11.4 -->
        <script src="https://code.jquery.com/ui/1.11.4/jquery-ui.min.js"></script>

         <script>
            $.widget.bridge('uibutton', $.ui.button);
        </script>

        <!-- Bootstrap 3.3.5 -->
        <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>

        <!-- cloudflare -->
        <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.10.2/moment.min.js"></script>

        <!-- Bootstrap WYSIHTML5 -->
        <script src="~/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.all.min.js"></script>

        <!-- AdminLTE App -->
        <script src="~/lib/AdminLTE/dist/js/app.min.js"></script>

        @*<script src="~/lib/jquery/dist/jquery.min.js"></script>*@
        @*<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>*@
        <script src="~/js/site.js" asp-append-version="true"></script>

        <script>
            $(function () {
                $('[data-toggle="tooltip"]').tooltip()
            })
        </script>

        @await RenderSectionAsync("Scripts", required: false)
    </body>
</html>
